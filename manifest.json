{
  "manifest_version": 2,
  "name": "ANT BundySW remote",
  "description": "Simple remote for BundySW",
  "version": "0.2",
  "permissions": [
    "webRequest",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "disabled.png",
    "default_title": "BundySW remote"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}